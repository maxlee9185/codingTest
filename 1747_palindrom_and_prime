#include <stdio.h>
#include <string.h>
#define MAXN 10000000
int N;
int eratos [MAXN];
int idx=1;
char s1[10000000];
void InputData() {
	scanf("%d", &N);
}

int isPalindrome(int target) {
	sprintf(s1, "%d", target);
	int len = strlen(s1);
	int flag = 1;
	for (int i = 0; i < len / 2; i++) {
		if (s1[i] != s1[len - 1 - i]) {
			flag = 0;
			break;
		}
	}
	if (flag == 1)return 1;
	return 0;
}

void Eratos() {
	eratos[0] = eratos[1] = 1;
	for (int i = 2; i <= MAXN; i++) {
		if (eratos[i] == 0) {
			for (int j = i+i; j <= MAXN; j = j+i) {
				eratos[j] = 1;
			}
		}
	}
}
void Solve() {
	int ans = N;
	Eratos();
	for (;;) {
		if (!eratos[ans] && isPalindrome(ans)) {
			printf("%d", ans);
			break;
		}
		else ans++;
	}
}
int main() {
	InputData();
	Solve();
	return 0;
}
